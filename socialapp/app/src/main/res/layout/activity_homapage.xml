<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homapageVM" type="com.socialapp.app.modules.homapage.data.viewmodel.HomapageVM"/>
    </data>
    <LinearLayout android:id="@+id/linearGroup8"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <TextView android:ellipsize="end" android:id="@+id/txtAppList"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_118pxh"
            android:layout_marginStart="@dimen/_118pxh"
            android:layout_marginTop="@dimen/_47pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{homapageVM.homapageModel.txtAppList}"
            style="@style/txtPoppinsbold28" tools:text="@string/lbl_app_list"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup9"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearGroup4"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_weight="0.6" android:layout_width="0dp" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearGroup2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLogofacebookpn"
                        android:layout_height="@dimen/_90pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_width="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_logofacebookpn}" tools:src="@drawable/img_logofacebookpn"/>
                    <ImageView android:id="@+id/imageLogoinstagram"
                        android:layout_height="@dimen/_90pxh"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_90pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_logoinstagram}" tools:src="@drawable/img_logoinstagram"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearGroup3"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_39pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageChromelogo1"
                        android:layout_height="@dimen/_82pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="@dimen/_98pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_chromelogo1}" tools:src="@drawable/img_chromelogo1"/>
                    <ImageView android:id="@+id/image1748571"
                        android:layout_height="@dimen/_81pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="@dimen/_82pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_1748571}" tools:src="@drawable/img_1748571"/>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameGroup11"
                android:layout_gravity="top"
                android:layout_height="@dimen/_65pxv"
                android:layout_marginBottom="@dimen/_133pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_134pxh">
                <FrameLayout android:id="@+id/frameGroup13"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_42pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_123pxh">
                    <ImageView android:id="@+id/imageLayer2"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_42pxh"
                        android:layout_width="@dimen/_123pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_layer2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCb"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:layout_marginStart="@dimen/_37pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{homapageVM.homapageModel.txtCb}"
                        style="@style/txtPoppinsbold28_1" tools:text="@string/lbl_cb"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageRectangle32"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_65pxh"
                    android:layout_width="@dimen/_134pxh" android:scaleType="fitXY"/>
            </FrameLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup5"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginTop="@dimen/_140pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnAbout"
                android:layout_height="@dimen/_44pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_width="@dimen/_100pxh"
                android:paddingBottom="@dimen/_12pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_29pxh"
                android:paddingTop="@dimen/_12pxv"
                android:singleLine="true"
                android:text="@string/lbl_about"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_about"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDevelopedByAn"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_62pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{homapageVM.homapageModel.txtDevelopedByAn}"
                style="@style/txtPoppinsbold17" tools:text="@string/msg_developed_by_an"/>
        </LinearLayout>
    </LinearLayout>
</layout>
